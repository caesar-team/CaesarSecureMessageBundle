{
  "name": "caesar/security-message-bundle",
  "description": "Security Message Bundle",
  "type": "symfony-bundle",
  "license": "proprietary",
  "require": {
    "php": "^7.2.5",
    "nelmio/api-doc-bundle": "^3.6",
    "ramsey/uuid": "^3.8.0",
    "symfony/console": "4.4.7",
    "symfony/dotenv": "4.4.7",
    "symfony/flex": "1.6.2",
    "symfony/form": "4.4.7",
    "symfony/framework-bundle": "4.4.7",
    "symfony/serializer-pack": "^1.0",
    "symfony/validator": "4.4.7",
    "symfony/yaml": "4.4.7"
  },
  "require-dev": {
    "ext-json": "*",
    "phpunit/phpunit": "8.5.3",
    "symfony/phpunit-bridge": "4.4.7"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Caesar\\SecurityMessageBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Caesar\\SecurityMessageBundle\\Tests\\": "tests/"
    }
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "4.4.7"
    }
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd"
    }
  }
}
