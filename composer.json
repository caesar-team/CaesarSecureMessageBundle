{
    "name": "4xxi/security-message",
    "description": "Security Message Bundle",
    "type": "symfony-bundle",
    "license": "proprietary",
    "require": {
        "php": "^7.2.5",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-redis": "*",
        "nelmio/api-doc-bundle": "^3.6",
        "predis/predis": "^1.1",
        "ramsey/uuid": "^3.8.0",
        "symfony/console": "4.4.7",
        "symfony/dotenv": "4.4.7",
        "symfony/flex": "1.6.2",
        "symfony/form": "4.4.7",
        "symfony/framework-bundle": "4.4.7",
        "symfony/serializer-pack": "^1.0",
        "symfony/validator": "4.4.7",
        "symfony/yaml": "4.4.7"
    },
    "require-dev": {
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Fourxxi\\SecurityMessageBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fourxxi\\SecurityMessageBundle\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-php72": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.7"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd"
        }
    }
}
